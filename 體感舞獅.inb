Peripheral Ser As ServoRunnerA @ 0
Peripheral myT As RF24G @ 8     '	Set the module ID as 8
Dim I As Byte
Dim Command As Dword
Dim CID As Byte
Dim Baudmode As Word
Dim Baudmode2 As Word
Dim POX,POY1,POY2 As WORD
Dim Data(2) As Byte
Dim DataX,DataY,DataZ As Word
Dim x As Byte
Dim g_TxReady As Byte 			' 	Declare the variable for the transmission status
#DEFINE RxPin 15				
#DEFINE txPin 14
#DEFINE Inverted 16384
#DEFINE Baudrate 9600 			'	Declare baudrate 


'-------------------------------Kinect----------------------------------
Sub Main()
	'Data = 0
	'Dim bTxData, bRxData As Byte ' Declare the variable for the received data and the transmission status
	Baudmode=(4096-(2000000\BaudRate))' 設定傳輸模式為38400bps
 	'Baudmode2=(4096-(2000000\BaudRate))+Inverted
	RunFrameWithTime(0,1000)
	'RunFrameWithTime(0,1000)
	ConfigInitVal()
Do	
	Serin RxPin,Baudmode,[Data]	
	Debug CSRXY(5,13), "Data(0)= ",Data(0),CLREOL
	Debug CSRXY(5,13), "Data(1)= ",Data(1),CLREOL
	DataX=Data(0)
	DataY=Data(1)
	
	If DataX > 0 AND DataX < 9 Then
		If DataY >8 And DataY <17 Then
		DataZ = DataX+(DataY*10)
		myT.SendVar(DataZ)
		TEST()
		End If
	End If
	If DataX=48 Then
		myT.SendVar(DataX)
		RunFrameWithTime(0,1000)
	End If
Loop

End Sub

Sub ConfigInitVal()
	'myT.EnTxReadyEvent() ' Enable the transmission complete notification Event
	'myT.SetMode(0) ' Set the mode as receiving mode
	'myT.SetCh(0) ' Set the transmission channel as 0
	myT.SetMode(0) ' Set the mode As transmitting mode
	myT.SetCh(30) '  Set the transmission channel as 0
	myT.Config()
End Sub

SUB TEST()
POX=(DataX-1)*150
POY1=(DataY-9)*106
POY2=(DataY-9)*89
Ser.SetPosSpdAndRun(0,2100-poX,1100)
Ser.SetPosSpdAndRun(3,2000-poY1,1100)
Ser.SetPosSpdAndRun(1,900+poY2,1100)
END SUB
