'======執行 "ID" 指定動作，同時設定等待時間====

'============================


Sub r()
		High(8)
		High(0)
End Sub
Sub g()
		High(9)
		High(1)
End Sub
Sub b()
		High(10)
		High(2)
End Sub
Sub rg()
		High(8)High(9)
		High(0)High(1)
End Sub
Sub gb()
		High(9)High(10)
		High(1)High(2)
End Sub
Sub rb()
		High(8)High(10)
		High(0)High(2)
End Sub
Sub rgb()
		High(8)High(9)High(10)
		High(0)High(1)High(2)
End Sub
Sub non()
		Low(8)Low(9)Low(10)
		Low(0)Low(1)Low(2)
End Sub
Sub RunFrameWithTime(bID As Byte,wTime As Word)
	Ser.LoadFrame(bID)	'載入Ser "ID" 指定動作
	Ser.runallservo() 	'執行Ser動作
	Pause wTime
End Sub

Sub RunFrame1(bID As Byte,wTime As Word)
	Ser.LoadFrame(bID)	'載入Ser "ID" 指定動作
	Ser.run1servo(0) 	'執行Ser動作
	Pause wTime
End Sub

Sub RunFrame4(bID As Byte,wTime As Word)
	Ser.LoadFrame(bID)	'載入Ser "ID" 指定動作
	Ser.run4servo(1,2,3,4) 	'執行Ser動作
	Pause wTime
End Sub

Sub RunFrame24(bID As Byte,wTime As Word)
	Ser.LoadFrame(bID)	'載入Ser "ID" 指定動作
	Ser.run2servo(2,4) 	'執行Ser動作
	Pause wTime
End Sub